namespace math;

fun int fibonacci(int n)
{
	if (n <= 1)
	{
		return n;
	}
	
	var int prev = 0;
	var int result = 1;
	
	for (var int i = 2; i <= n; i = i + 1)
	{
		let int sum = result + prev;
		prev = result;
		result = sum;
	}
	
	return result;
}

fun int fibonacci_recursive(int n)
{
	if (n <= 1)
	{
		return n;
	}
	else
	{
		return fibonacci_recursive(n - 1) + fibonacci_recursive(n - 2);
	}
}